// This is a test file for if statement in joker language.

if (true) {
    print true;                 //  this statement will be executed.
} else {
    print false;
}

var x = 10;
if (x < 5) {
    print "x is less than 5";
} else if (x > 5) {             //  this statement will be executed.
    print "x is greater than 5";
} else {
    print "x is equal to 5";
}

if (x > 10) {
    print "x is greater than 10";
} else if (x < 10) {
    print "x is less than 10";  
} else {
    print "x is equal to 10";   //  this statement will be executed.
}

